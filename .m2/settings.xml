<settings>
    <pluginGroups>
        <pluginGroup>org.sonarsource.scanner.maven</pluginGroup>
    </pluginGroups>
    <profiles>
        <profile>
            <id>sonar</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <sonar.host.url>
                    https://sonarcloud.io
                </sonar.host.url>
                <sonar.coverage.exclusions>
                    **/model/**/*, **/mapper/**/*, **/*JPA.*,**/Page.*,**/SpringPageMapper.*,
                    **/config/*,**/error/*,**/filter/*,**/*EntryPoint, **/ZerofiltreBlogApplication.*, **/util/*,
                    **/*Exception.*,**/logging/**/*,**/InfraProperties.*
                </sonar.coverage.exclusions>
                <sonar.projectKey>Zerofiltre-Courses_zerofiltre-blog-backend</sonar.projectKey>
                <sonar.organization>zerofiltre-courses</sonar.organization>
            </properties>
        </profile>
    </profiles>
</settings>


